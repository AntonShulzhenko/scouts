- var nav = ['О нас', 'Наши лидеры', 'Фотогалерея', 'Как стать скаутом', 'Библиотека', 'Контакты'];

header.header
  a(href="#" class="header__logo logo")
    img.logo__img(src="img/logo.png" alt="Ассоциация днепровских скаутов «СКИФ»")
    span.logo__text Ассоциация днепровских скаутов «СКИФ»

  nav.nav
    ul.nav__list
      each item in nav
        li.nav__item
          a(href="#" class="nav__link") #{item}
          
  .header__contacts
    p.header__address Украина, г. Днепропетровск, ул. Артема 1й
    a.header__email(href="mailto:adsskif@mail.ru") adsskif@mail.ru
